    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "cookie-signature": "^1.2.2",
    "next": "^15.4.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
